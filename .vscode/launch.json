{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Web",
            "request": "launch",
            "type": "chrome",
            "webRoot": "${workspaceFolder}/src/web/src",
            "url": "http://localhost:5173",
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            },
        }
    ],
    "inputs": [
        {
            "id": "dotEnvFilePath",
            "type": "command",
            "command": "azure-dev.commands.getDotEnvFilePath"
        }
    ]
}